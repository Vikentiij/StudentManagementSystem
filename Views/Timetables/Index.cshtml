@model IEnumerable<StudentManagementSystem.Models.Timetable>

@{
    ViewData["Title"] = "Timetables";
    Layout = "~/Views/Shared/DashboardLayout.cshtml";
}

<h1>Timetable</h1>



@if (this.User.IsInRole("Admin"))
{
    <p>
        <a asp-action="Create">Create New</a>
    </p>
}

@if ((this.User.IsInRole("Teacher") || this.User.IsInRole("Student")) && !this.User.IsInRole("Admin"))
{
    <br />
}
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Day)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Course)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Room)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Day)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Course.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Room.Name)
                </td>
                @if (this.User.IsInRole("Admin"))
                {
                    <td>
                        @*  <a asp-action="Edit" asp-route-id="@item.TimetableId">Edit</a> |
                            <a asp-action="Details" asp-route-id="@item.TimetableId">Details</a> |*@
                        <a asp-action="Delete" asp-route-id="@item.TimetableId">Delete</a>
                    </td>
                }
            </tr>
        }
    </tbody>
</table>




<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/fullcalendar/3.0.1/fullcalendar.min.css" />
<h2>Calendar</h2>
<div id="calendar">

</div>
@section Scripts {
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.29.1/moment.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="https://fullcalendar.io/js/fullcalendar-2.2.5/fullcalendar.min.js"></script>
    <script>

        $(document).ready(function () {
            var settings = {
                "url": "/api/timetables/",
                "method": "GET",
                "timeout": 0,
            };

            $.ajax(settings).done(function (response) {
                 $('#calendar').fullCalendar({

                    header: {
                        left: 'prev,next today',
                        center: 'title',
                        right: 'month,agendaWeek,agendaDay'
                    },
                    defaultView: 'month',
                    eventColor: '#378006',

                    events: response
                })
            });


            

        })
    </script>
}




